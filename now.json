{
  "name": "velox-news-api",
  "version": 2,
  "scope": "velox",
  "public": false,
  "alias": ["velox-api"],
  "builds": [
    { "src": "www/**/*", "use": "@now/static" },
    { "src": "api/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/", "dest": "www/index.html" },

    {
      "src": "/.*",
      "headers": {
        "Access-Control-Allow-Origin": "https://velox.cc"
      }
    }
  ]
}
